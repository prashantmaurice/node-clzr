<div class="container">
      <h2>Form</h2>
      <form role="form" action="create" method="POST">
        <div class="form-group">
          <label for="Vendor id">Vendor name:</label>
          <input type="text" class="form-control" name="ID" placeholder="Enter Vendor ID">
        </div>
        <div class="form-group">
          <label for="Vendor name">Vendor name:</label>
          <input type="text" class="form-control" name="name" placeholder="Enter Vendor name">
        </div>
        <div class="form-group">
          <label for="Activeness">Activeness:</label>
          <input type="text" class="form-control" name="activeness" placeholder="Enter activeness">
        </div>
         <div class="form-group">
          <label for="Tags">Tags:</label>
          <input type="text" class="form-control" name="tags" placeholder="Enter Tags">
        </div>
        <div class="form-group">
          <label for="Rating">Rating:</label>
          <input type="text" class="form-control" name="rating" placeholder="Enter Rating">
        </div>
        <div class="form-group">
          <label for="Rating count">Rating count:</label>
          <input type="text" class="form-control" name="ratingcount" placeholder="Enter Rating count">
        </div>
        <input type="hidden" name="_csrf" value="<%=_csrf%>"/>
        <button type="submit" class="btn btn-default">Submit</button>
      </form>
    </div>
