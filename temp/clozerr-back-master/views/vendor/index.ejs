<div class="container">
<h3>Coupons</h3>
<table class='table'>
<tr>
	<th>Vendor ID</th>
	<th>Vendor Name</th>
	<th>Vendor activeness</th>
	<th>Rating</th>
	<th>Rating count</th>
</tr>

<% _.each(vendors,function(vendor) { %>

<tr data-id="<%= vendor.id %>" data-model="<%= vendor %>">
	<td><%= vendor.id %></td>
	<td><%= vendor.name%></td>
	<td><%= vendor.activeness%></td>
	<td><%= vendor.rating %></td>
	<td><%= vendor.ratingcount %></td>
    <td><a href="/vendor/show/<%=vendor.id%>"class="btn btn-default">show</a></td>
	<td><a href="/vendor/edit/<%=vendor.id%>"class="btn btn-default">edit</a></td>
	<td><a href="/vendor/destroy/<%=vendor.id%>"class="btn btn-default">destroy</a></td>
</tr>
<% }) %>
</table>
</div>