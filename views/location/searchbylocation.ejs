<div class="container">
      <h2>Form</h2>
      <form role="form" action="locate" method="POST">
        
        <div class="form-group">
          <label for="Longitude">Longitude:</label>
          <input type="float" class="form-control" name="longitude" placeholder="Enter longitude">
        </div>
        <div class="form-group">
          <label for="Latitude">Latitude:</label>
          <input type="float" class="form-control" name="latitude" placeholder="Enter latitude">
        </div>
        <input type="hidden" name="_csrf" value="<%=_csrf%>"/>
        <button type="submit" class="btn btn-default">Submit</button>
      </form>
    </div>
