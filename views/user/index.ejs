<div class="container">
<h3>Users</h3>
<table class='table'>
<tr>
	<th>Name</th>
	<th>Email</th>
</tr>

<% _.each(users,function(user) { %>

<tr data-id="<%= user.id %>" data-model="<%= user %>">
	<td><%= user.id %></td>
	<td><%= user.name %></td>
	<td><%= user.email %></td>
</tr>
<% }); %>
</table>
</div>